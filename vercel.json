{
    "builds": [
        {
          "src": "text_summarizer_frontend/app.py",
          "use": "@vercel/python"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "text_summarizer_frontend/app.py"
        }
      ],
      "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "text_summarizer_frontend/app.py"
      }
}